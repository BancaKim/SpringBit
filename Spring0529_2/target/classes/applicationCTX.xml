<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="bmiCalculator" class="com.kbstar.digi.BMICalculator">
<property name="lowWeight" value="18.5"/>
      <property name="normal" value="23"/>
      <property name="overWeight" value="25"/>
      <property name="obesity" value="30"/>
   </bean>

<bean id="myInfo" class="com.kbstar.digi.MyInfo">
	<property name="bmiCalculator">
		<ref bean="bmiCalculator"/>
	</property>
	<property name="name" value="김건"/>
	<property name="height" value="181"/>
	<property name="weight" value="78"/>
	
	<property name="hobbys">
		<list>
			<value>코딩</value>
		</list>
	</property>
</bean>

</beans>
